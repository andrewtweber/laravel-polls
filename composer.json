{
    "name": "andrewtweber/laravel-polls",
    "type": "library",
    "description": "Polls package for Laravel",
    "keywords": [
        "laravel",
        "polls",
        "poll"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Weber",
            "homepage": "https://andrew.cool"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/andrewtweber/slimak.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "php": "^8.2",
        "andrewtweber/slimak": "*",
        "laravel/framework": "^11.0|^12.0",
        "leth/ip-address": "^2.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Andrewtweber\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andrewtweber\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andrewtweber\\Providers\\PollsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
