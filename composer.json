{
    "name": "andrewtweber/laravel-polls",
    "type": "library",
    "description": "Polls package for Laravel",
    "keywords": [
        "laravel",
        "polls",
        "poll"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Weber",
            "homepage": "https://andrew.cool"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/andrewtweber/slimak.git"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "php": "^8.1",
        "andrewtweber/slimak": "*",
        "laravel/framework": "^8.12|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.17",
        "phpunit/phpunit": "^9"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Andrewtweber\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Andrewtweber\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andrewtweber\\Providers\\SnaccsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
